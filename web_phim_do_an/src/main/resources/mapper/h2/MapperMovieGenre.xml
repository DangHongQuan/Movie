<!-- movie_mapper.xml -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.response.admin.AdminMovieGenreRepository">
	<resultMap id="genresResultMap" type="com.example.models.admin.MovieGenre">
		<id property="mgId" column="MG_ID" />
		<result property="movieId" column="MOVIE_ID" />
		<result property="genreId" column="GENRE_ID" />
	</resultMap>

	<insert id="insertMovieGenres">
		INSERT INTO moviegenre (MOVIE_ID, GENRE_ID)
		VALUES ( #{movieId}, #{genreId} )
	</insert>

</mapper>
